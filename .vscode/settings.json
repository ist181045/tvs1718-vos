{
  "latex-workshop.latex.outputDir": "../../../target/tex",
  "latex-workshop.latex.tools": [
    {
      "name": "pdflatex",
      "command": "pdflatex",
      "args": [
        "-shell-escape",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-output-directory=../../../target/tex",
        "%DOC%"
      ]
    },
  ],
}